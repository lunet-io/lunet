{{~ for $meta in site.html.head.metas ~}}
    {{ object.eval $meta }}
{{~ end ~}}
    <title>{{ if site.html.head.title; site.html.head.title; else if page.url == '/'; site.title; else; page.title + " - " + site.title; end; }}</title>
{{~ for $include in site.html.head.includes ~}}
    {{~ include $include ~}}
{{~ end ~}}
