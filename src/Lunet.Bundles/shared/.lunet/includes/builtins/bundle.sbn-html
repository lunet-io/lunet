{{~ # Generates bundle links ~}}
{{~ for $link in (bundle page.bundle).links ~}}
    {{~ if $link.type == "css" ~}}
    <link rel="stylesheet" href="{{$link.url}}">
    {{~ end ~}}
{{~ end ~}}
{{~ for $link in (bundle page.bundle).links ~}}
    {{~ if $link.type == "js" ~}}
    <script src="{{$link.url}}" type="text/javascript" {{if $link.mode != ""; $link.mode; end }}></script>
    {{~ end ~}}
{{~ end ~}}